    if (_mode == undefined) {
        _mode = CATSPEAK_EXT_GML_ALL;
    }
    if (_mode & CATSPEAK_EXT_GML_FUNCTIONS) {
        var f = catspeak_session_add_function;
        f(_session, "weak_ref_create", weak_ref_create);
        f(_session, "weak_ref_alive", weak_ref_alive);
        f(_session, "weak_ref_any_alive", weak_ref_any_alive);
        f(_session, "ptr", ptr);
    }
    if (_mode & CATSPEAK_EXT_GML_CONSTANTS) {
        var c = catspeak_session_add_constant;
        c(_session, "pointer_invalid", pointer_invalid);
        c(_session, "pointer_null", pointer_null);
    }
}

